<taglib version="2.1" 
	xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xml="http://www.w3.org/XML/1998/namespace" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_1.xsd ">
  <tlib-version>1.0</tlib-version>
  <short-name>message</short-name>
  <uri>http://www.simpliccity.org/sst/webflow/validation</uri>
  <description>
  	A set of simple tags for working with data validation
  	using Spring Web Flow.
  </description>
  <tag-file>
  	<name>messages</name>
  	<path>/META-INF/tags/configuration/messages.tag</path>
  </tag-file>
</taglib>
